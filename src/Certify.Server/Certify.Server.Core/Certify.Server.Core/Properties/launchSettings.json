{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://127.0.0.2:9695"
    }
  },

  "profiles": {

    "Certify.Server.Core": {
      "commandName": "Project",
      "launchBrowser": false,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://127.0.0.2:9695",
        "CERTIFY_MANAGEMENT_HUB": "https://localhost:44361/api/internal/managementhub"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false,
      "launchUrl": "http://127.0.0.2:9695/docs",
      "iisExpress": {
        "applicationUrl": "http://127.0.0.2:9695"
      },
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/docs",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://0.0.0.0:9695"
      },
      "publishAllPorts": true,
      "httpPort": 9695
    },
    "WSL": {
      "commandName": "WSL2",
      "launchBrowser": false,
      "launchUrl": "http://localhost:9695/docs",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
